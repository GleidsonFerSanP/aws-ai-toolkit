{
  "name": "aws-ai-toolkit",
  "displayName": "AWS AI Toolkit",
  "description": "Unified AWS management for AI agents via MCP - Control EC2, RDS, DynamoDB, Lambda, ECS, EKS and more through GitHub Copilot",
  "version": "2.0.3",
  "publisher": "GleidsonFerSanP",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/GleidsonFerSanP/mcp-aws-cli.git"
  },
  "bugs": {
    "url": "https://github.com/GleidsonFerSanP/mcp-aws-cli/issues"
  },
  "homepage": "https://github.com/GleidsonFerSanP/mcp-aws-cli#readme",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "mcpServerDefinitionProviders": [
      {
        "id": "mcp-aws-cli",
        "label": "MCP AWS CLI"
      }
    ],
    "commands": [
      {
        "command": "mcpAwsCli.showLogs",
        "title": "MCP AWS CLI: Show Logs"
      },
      {
        "command": "mcpAwsCli.clearCache",
        "title": "MCP AWS CLI: Clear Cache"
      },
      {
        "command": "mcpAwsCli.reloadConfig",
        "title": "MCP AWS CLI: Reload Configuration"
      },
      {
        "command": "mcpAwsCli.showInfo",
        "title": "MCP AWS CLI: Show Server Info"
      }
    ],
    "configuration": {
      "title": "MCP AWS CLI",
      "properties": {
        "mcpAwsCli.defaultRegion": {
          "type": "string",
          "default": "us-east-1",
          "description": "Default AWS region"
        },
        "mcpAwsCli.cacheTimeout": {
          "type": "number",
          "default": 300,
          "description": "Cache timeout"
        },
        "mcpAwsCli.enableDebugLogs": {
          "type": "boolean",
          "default": false,
          "description": "Debug logs"
        },
        "mcpAwsCli.maxRetries": {
          "type": "number",
          "default": 3,
          "description": "Max retries"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run clean && tsc -p ./",
    "clean": "rm -rf dist",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-server",
    "aws",
    "amazon-web-services",
    "cloud",
    "ai",
    "copilot",
    "github-copilot",
    "ec2",
    "rds",
    "dynamodb",
    "lambda",
    "ecs",
    "eks",
    "cloudwatch",
    "infrastructure",
    "devops",
    "automation"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-account": "^3.970.0",
    "@aws-sdk/client-cloudwatch": "^3.700.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.700.0",
    "@aws-sdk/client-cost-explorer": "^3.700.0",
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/client-ec2": "^3.700.0",
    "@aws-sdk/client-ecs": "^3.700.0",
    "@aws-sdk/client-eks": "^3.700.0",
    "@aws-sdk/client-iam": "^3.700.0",
    "@aws-sdk/client-lambda": "^3.700.0",
    "@aws-sdk/client-rds": "^3.700.0",
    "@aws-sdk/client-rds-data": "^3.970.0",
    "@aws-sdk/client-resource-groups-tagging-api": "^3.970.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-secrets-manager": "^3.970.0",
    "@aws-sdk/client-service-quotas": "^3.700.0",
    "@aws-sdk/client-ssm": "^3.970.0",
    "@aws-sdk/client-sts": "^3.700.0",
    "@aws-sdk/credential-provider-env": "^3.970.0",
    "@aws-sdk/credential-provider-ini": "^3.971.0",
    "@aws-sdk/credential-provider-node": "^3.971.0",
    "@aws-sdk/credential-provider-process": "^3.970.0",
    "@aws-sdk/credential-provider-sso": "^3.971.0",
    "@aws-sdk/util-dynamodb": "^3.970.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^2.22.0",
    "typescript": "^5.3.0"
  }
}
